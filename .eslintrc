{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-alert": "error",
    "no-console": "off",
    "semi": "warn",
    "no-empty-function": ["error", { "allow": ["methods", "constructors"] }]
  }
}